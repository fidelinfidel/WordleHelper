<Window x:Class="WordleSovlerHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordleSovlerHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="FindSolutions" HorizontalAlignment="Left" Margin="673,384,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="PossibleWords" HorizontalAlignment="Left" Margin="30,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="120" Height="373"/>
        <TextBox x:Name="ValidLetters" HorizontalAlignment="Left" Margin="358,354,0,0" TextWrapping="Wrap" Text="q,w,e,r,t,y,u,i,o,p,a,s,d,f,g,h,j,k,l,z,x,c,v,b,n,m" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="Valid Letters" HorizontalAlignment="Left" Margin="362,323,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="YellowLetters" HorizontalAlignment="Left" Margin="362,287,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Yellow Letters" HorizontalAlignment="Left" Margin="362,256,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.502,0.518"/>
        <Label Content="Possible Words" HorizontalAlignment="Left" Margin="45,5,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Unknowns" HorizontalAlignment="Left" Margin="362,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged" Height="168"/>
        <Label Content="Unknowns" HorizontalAlignment="Left" Margin="357,18,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.502,0.518"/>

    </Grid>
</Window>
